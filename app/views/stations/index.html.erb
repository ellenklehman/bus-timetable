<div class="container">
  <div class="page-header">
    <h1>Stations</h1>
  </div>
  <div id="alchemy" class="alchemy"></div>
  <script type="text/javascript">
    var some_data =
      {
        "nodes": [
          {
            "id": 1
          },
          {
            "id": 2
          },
          {
            "id": 3
          }
        ],
        "edges": [
          {
            "source": 1,
            "target": 2
          },
          {
            "source": 1,
            "target": 3,
          }
        ]
      };
    alchemy.begin({"dataSource": some_data})
  </script>

  <ul>
    <% @stations.each do |station| %>
      <li><%= link_to "#{station.name}", station_path(station) %></li>
    <% end %>
  </ul>

  <p><%= link_to "Add new station", new_station_path, :class => "btn btn-primary" %></p>
</div>
